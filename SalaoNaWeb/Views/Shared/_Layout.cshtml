<!DOCTYPE html>
<html lang="pt">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - SalaoWeb</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <p class="navbar-brand"><a href="http://localhost:54024"><img src="data:image/png;base64," width="140" class="img-responsive" height="20" /></a></p>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    @*<li>@Html.ActionLink("Cidades", "Index", "Cidade", new { area = "" }, null)</li>*@
                    <li>@Html.ActionLink("Empresas", "Index", "Empresa", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("Clientes", "Index", "Cliente", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("Funcionarios", "Index", "Funcionario", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("Servicos", "Index", "Servico", new { area = "" }, null)</li>
                    @*<li>@Html.ActionLink("Agenda", "Index", "Agenda", new { area = "" }, null)</li>*@
                    <li>@Html.ActionLink("Horarios Agendados", "Index", "Calendario", new { area = "" }, null)</li>
                </ul>
                @*<ul class="nav navbar-nav navbar-right" data-bind="with: user">
                    <li data-bind="click: logOff">@Html.ActionLink("Fazer logoff", null, null, new { @class = "glyphicon glyphicon-off" })
                </ul>*@
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p class="input-group">&copy; @DateTime.Now.Year - Desenvolvido por  <a href="http://www.una.br/">Una Barreiro</a>. Todos direitos reservados. &copy;</p>
        </footer>
    </div>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="http://maps.google.com/maps/api/js?sensor=false"></script>
    <script src="~/Scripts/dhtmlxScheduler/ext/dhtmlxscheduler_map_view.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/datetime")
    @RenderSection("Scripts", required: false)
</body>
</html>
