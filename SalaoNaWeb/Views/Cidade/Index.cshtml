@model IEnumerable<SalaoNaWeb.Models.Cidade>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cidNom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cidUf)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cidNom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cidUf)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.cidId }) |
            @Html.ActionLink("Details", "Details", new { id=item.cidId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.cidId })
        </td>
    </tr>
}

</table>
