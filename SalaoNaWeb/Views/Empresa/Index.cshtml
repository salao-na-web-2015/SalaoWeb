@model IEnumerable<SalaoNaWeb.Models.Empresa>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cidade.cidNom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.razSoc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nomFant)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cnpj)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cidade.cidNom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.razSoc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nomFant)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cnpj)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.empId }) |
            @Html.ActionLink("Details", "Details", new { id=item.empId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.empId })
        </td>
    </tr>
}

</table>
